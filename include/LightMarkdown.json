{
	"keywords": {
		"^[-/] ": "#EEEE22",
		"^(?:\\t|    )[-/] ": "#F56545",
		"^(?:\\t|    ){2}[-/] ": "#0CBDE8",
		"^(?:\\t|    ){3}[-/] ": "#0DFF64",
		"^[-=]+$": {"bold": true},
		"^\\s*#+ [^#]+#*$": {"colour": "lightsteelblue", "bold": true},
		"([*_]{2})(?:\\w|\\w[^*_]*\\w)\\1": {"bold": true},
		"([*_])(?:\\w|\\w[^*_]*\\w)\\1": {"italics": true}
	}
}