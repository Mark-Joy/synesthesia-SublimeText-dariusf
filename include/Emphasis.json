{
	"keywords": {
		"^\\s*#+ [^#]+#*$": {"colour": "#70C5EF", "bold": true},
		"([*]{1,2})(?:\\w|\\w[^*_`\"]*\\w)\\1": {"colour": "#FFE24C", "bold": true},
		"([_]{1,2})(?:\\w|\\w[^*_`\"]*\\w)\\1": "#F94B97",
		"([`])(?:\\w|\\w[^*_`\"]*\\w)\\1": {"colour": "#38FF81", "italics": true},
		"([\"])(?:\\w|\\w[^*_`\"]*\\w)\\1": "#9A6EE6",
		"\\[(?:\\w[^\\[\\]]*[^ ])\\]": "#FFE24C"
	}
}